<%= form_with(model: lock) do |form| %>
  <% if lock.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(lock.errors.count, "error") %> prohibited this lock from being saved:</h2>

      <ul>
        <% lock.errors.each do |error| %>
          <li><%= error.full_message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :number %>
    <%= form.number_field :number %>
  </div>

  <div class="field">
    <%= form.label :company_id %>
    <%= form.text_field :company_id %>
  </div>

  <div class="field">
    <%= form.label :property_id %>
    <%= 
    properties =Property.all
    selected_prop=[]
    properties.each do |prop|
     if @lock.company_id == prop.company_id
        selected_prop.push(prop)
     end
    end
    form.collection_select(:property_id,selected_prop, :id, :name)
    %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
